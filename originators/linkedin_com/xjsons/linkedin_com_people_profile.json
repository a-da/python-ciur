{
    "name": "linkedin_com_people_profile",
    "test_url" : "http://www.linkedin.com/in/pavelfusu",
    "version": 0,
    "versions": [{
        "version" : 0,
        "timestamp" : 1330934431,
        "config" : {
            "xpath" : {
                "root" : "/html:html/html:body",
                "namespaces" : {
                    "html": "http://www.w3.org/1999/xhtml"
                }
            },
            "anomaly" : {
                "root" : "/html:html/html:body",
                "namespaces" : {

                }
            }
        },
        "anomaly" : {},
        "rules" : {
            "first_name": ["text.|mandatory", "//html:*[@class='given-name']"],
            "last_name" : ["text.|mandatory", "//html:*[@class='family-name']"],
            "picture": ["text.|optional", ".//html:*[@id='profile-picture']/html:img/@src"],
            "additional":["optional", ".//html:*[@id='profile-additional']/html:*[contains(@class,'content')]//html:dl",
                {
                    "interests":["text.|optional", "./html:dd[@class='interests']//html:p"],
                    "pubgroups": ["ensure_list|optional",
                        "./html:dd[@class='pubgroups']//html:*[contains(@class,'affiliation')]",
                        {
                            "group_logo":["text.|mandatory", ".//html:*[contains(@class,'group-logo')]//html:img/@src"],
                            "group_data":["text.|mandatory", ".//html:*[contains(@class,'group-data')]//html:strong"]
                        }],
                    "honors": ["text.|optional", "./html:dd[@class='honors']//html:p"],
                    "websites": ["ensure_list|optional",
                        "./html:dd[@class='websites']//html:li",
                        {
                            "href":["text.|mandatory", "./html:a/@href"],
                            "name":["text.|mandatory", "./html:a"]
                        }]
            }],
            "skills": ["text.|optional", "//html:*[@id='profile-skills']/html:*[contains(@class,'content')]//html:*[contains(@class,'competency')]//html:*[@class='jellybean']//text()"],
            "overview": ["optional", "//html:*[@id='overview']",
                {
                    "current": ["text.force_list|optional", "./html:dd[@class='summary-current']//html:li/text()"],
                    "conn_number": ["int.|optional", "./html:*[@class='overview-connections']//html:strong"],
                    "recommend": ["int.|optional", "./html:*[not(@class)]//html:strong"]
                }],
            "industry": ["text.|optional", "//html:*[@class='industry']"],
            "title": ["text.|mandatory", "//html:p[contains(@class,'title')]"],
            "experience": ["optional", "//html:*[@id='profile-experience']/html:*[contains(@class,'content')]",
                {
                    "current": ["ensure_list|optional", "./html:*[contains(@class,'current-position')]",
                        {
                            "name": ["text.|mandatory", ".//html:h4//text()"],
                            "title": ["text.|mandatory", ".//html:*[@class='title']"],
                            "current": ["text.|optional", ".//html:*[@class='period']/html:*[@class='dtstamp']/@title"],
                            "start": ["text.|optional", ".//html:*[@class='period']/html:*[@class='dtstart']/@title"],
                            "details": ["text.|mandatory", ".//html:*[@class='orgstats organization-details']"],
                            "ref": ["text.|optional", ".//html:*[@class='company-profile-public']/@href"],
                            "description": ["text.|optional", ".//html:*[contains(@class,'description')]//text()"]
                        }],
                    "past": ["ensure_list|optional", "./html:*[contains(@class,'past-position')]",
                        {
                            "end": ["text.|mandatory", ".//html:*[@class='period']/html:*[@class='dtend']/@title"],
                            "description": ["text.|optional", ".//html:*[contains(@class,'description')]//text()"],
                            "title": ["text.|mandatory", ".//html:*[@class='title']"],
                            "start": ["text.|mandatory", ".//html:*[@class='period']/html:*[@class='dtstart']/@title"],
                            "details": ["text.|mandatory", ".//html:*[@class='orgstats organization-details']"],
                            "name": ["text.|mandatory", ".//html:h4//text()"]
                        }],
                    "summary": ["optional", "//html:*[@id='profile-summary']/html:*[contains(@class,'content')]",
                        {
                            "specialties": ["text.|optional", ".//html:*[@id='profile-specialties']//html:*[@class='null']//text()"],
                            "description": ["text.|optional", ".//html:*[contains(@class, 'description')]//text()"]
                        }],
                    "languages": ["optional", "//html:*[@id='profile-languages']/html:*[contains(@class,'content')]//html:*[contains(@class,'competency')]",
                        {
                            "lang": ["text.|mandatory", ".//html:h3"],
                            "proficiency": ["text.|optional", ".//html:span"]
                        }]
                }],

            "education": ["ensure_list|optional", "//html:*[@id='profile-education']/html:*[contains(@class,'content')]//html:*[contains(@class,'position')]",
                {
                    "activities": ["text.|optional", ".//html:*[@name='activities']/text()"],
                    "major":["text.|optional", ".//html:*[contains(@class,'major')]"],
                    "name":["text.|mandatory", ".//html:*[contains(@class,'org')]"],
                    "degree":["text.|optional", ".//html:*[contains(@class,'degree')]"],
                    "period":["optional", ".//html:*[contains(@class,'period')]",
                        {
                            "dtend": ["text.|optional", ".//html:*[contains(@class,'dtend')]/@title"],
                            "dtstart": ["text.|optional", ".//html:*[contains(@class,'dtstart')]/@title"]
                        }],
                    "description": ["text.|optional", ".//html:*[contains(@class,'desc')]//text()"]
                }],
            "locality": ["text.|optional", "//html:*[@class='locality']"]
        },
        "light_handlers" : {
        },
        "renames" : {},
        "blocks" : {}
    }]
}