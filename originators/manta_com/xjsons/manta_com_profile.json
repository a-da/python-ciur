    "version": 0,
    "versions": [{
        "version" : 0,
        "timestamp" : 1330895625,
        "config" : {
            "xpath" : {
                "root" : "/html:html/html:body",
                "namespaces" : {
                    "html": "http://www.w3.org/1999/xhtml"
                }
            },
            "anomaly" : {
                "root" : "/html:html/html:body",
                "namespaces" : {

                }
            }
        },
        "anomaly" : {},
        "rules" : {
            "company_info":["mandatory", ".//html:div[@class='company-info']|.//html:div[@class='company-info edit-overlay-section LTpad']",
                {
                    "website": ["text.|^replace:information_not_found|optional", ".//html:dt[text()='Website:']/following-sibling::html:dd[1]|.//html:dt[text()='Website:']/following-sibling::html:dd[1]/html:span"],
                    "phone":["text.|optional", ".//html:*[@class='tel']"],
                    "org":["mandatory", ".//html:h1[@class='company-name']|.//html:h1[@class='profile-company_name']",
                        {
                            "alias":["text.|optional", "./../following-sibling::html:em[1]"],
                            "href":["text.|mandatory", "./../@href"],
                            "name":["text.|mandatory", "."]
                        }],
                    "address" : ["mandatory", "./html:div[@itemprop='address']",
                        {
                            "map":["text.|optional", "//html:*[@class='tabset']//html:a[@id='map-tab-link']/@href"],
                            "locality":["text.|mandatory", ".//html:*[@itemprop='addressLocality']"],
                            "post_office_box_nr": ["int.|optional", ".//html:*[@itemprop='postOfficeBoxNumber']"],
                            "country":["text.|optional", ".//html:*[@itemprop='addressCountry']"],
                            "region":["text.|optional", ".//html:*[@itemprop='addressRegion']"],
                            "adr_area":["text.|optional", ".//html:*[@class='adr-area']"],
                            "street":["text.|optional", "./html:div[@itemprop='streetAddress']"],
                            "postal_code":["text.|optional", ".//html:*[@class='addresspostalCode']"]
                        }]
                }],
            "about":["mandatory", ".//html:div[@class='about edit-overlay-section']|.//html:div[@class='about']",
                {
                    "detailed": ["text.|^replace:see|optional", ".//html:span[@id='profile-detailed_description']|.//html:p[@itemprop='description']"],
                    "short":["text.|optional", ".//html:strong[@id='profile-short_description']"]
                }],
            "contacts": ["ensure_list|optional", ".//html:ul[@class='items']/html:li",
                {
                    "employees":["mandatory", ".//html:*[@itemprop='employees']",
                        {
                            "url":["text.|mandatory", ".//html:*[@itemprop='url']/@href|.//html:a/@href"],
                            "job_title":["text.|mandatory", ".//html:em[@itemprop='jobTitle']"],
                            "email": ["text.|^replace:mail|optional", ".//html:p[@class='email']/html:script"],
                            "name":["text.|optional", ".//html:*[@itemprop='name']"]
                        }],
                    "img":["text.|mandatory", ".//html:img/@src"]
                }],
            "add_info": ["text.force_list|^replace:view_newly|mandatory",
                ".//html:ul[@class='add-info']//html:ul/html:li|.//html:ul[@class='add-info']//html:ul/html:span/html:li"],
            "table_data":["optional", ".//html:table[@class='table-data']",
                {
                    "psb": ["text.|^replace:information_not_found|optional",
                        ".//html:th[text()='Products, Services']/following-sibling::html:td[1]|.//html:th[text()='Products, Services']/following-sibling::html:td[1]/html:span"],
                    "years_business": ["text.|^replace:information_not_found|optional",
                        ".//html:th[text()='Years in Business']/following-sibling::html:td[1]|.//html:th[text()='Years in Business']/following-sibling::html:td[1]/html:span", "# format of int <1> or <Less than 1 >"],
                    "employees": ["text.|^replace:information_not_found|optional", ".//html:th[text()='Employees ']/following-sibling::html:td[1]|.//html:th[text()='Employees']/following-sibling::html:td[1]/html:span"],
                    "naics_code": ["text.|optional", ".//html:th[text()='NAICS Code']/following-sibling::html:td[1]|.//html:th[text()='NAICS Code']/following-sibling::html:td[1]/html:span"],
                    "sic_code": ["text.|^replace:information_not_found|optional", ".//html:th[text()='SIC Code']/following-sibling::html:td[1]|.//html:th[text()='SIC Code']/following-sibling::html:td[1]/html:span"],
                    "industry": ["text.|^replace:information_not_found|optional", ".//html:th[text()='Industry']/following-sibling::html:td[1]|.//html:th[text()='Industry']/following-sibling::html:td[1]/html:span"],
                    "annual_revenue":["text.|^replace:information_not_found|optional", ".//html:th[text()='Annual Revenue']/following-sibling::html:td[1]|.//html:th[text()='Annual Revenue']/following-sibling::html:td[1]/html:span"],
                    "annual_sales": ["text.|^replace:information_not_found|optional", ".//html:th[text()='Annual Sales ']/following-sibling::html:td[1]|.//html:th[text()='Annual Sales ']/following-sibling::html:td[1]/html:span"],
                    "state_inc": ["text.|^replace:information_not_found|optional", ".//html:th[text()='State of Incorporation']/following-sibling::html:td[1]|.//html:th[text()='State of Incorporation']/following-sibling::html:td[1]/html:span"],
                    "location_type":["text.|optional", ".//html:th[text()='Location Type']/following-sibling::html:td[1]|.//html:th[text()='Location Type']/following-sibling::html:td[1]/html:span"]
                }],
                "parent_urls":["mandatory", ".//html:ul[@itemprop='breadcrumb']//html:a", {
                    "href":["text.|mandatory", "./@href"],
                    "name":["text.|mandatory", "./html:span"]
                }]
        },
        "light_handlers" : {
            "^replace:information_not_found" : [{
                "from_str" : "Information not found",
                "to" : ""
            }],
            "^replace:see":[{
                "from_str": " Register for free to see additional information such as annual revenue and employment figures.",
                "to": ""
            }],
            "^html:":{},
            "^replace:mail":[{
                    "to": "",
                    "from_str": "wr(\""
                }, {
                    "to": "",
                    "from_str": ";"
                }, {
                    "to": "",
                    "from_str": "\")"
                }
            ],
            "^replace:view_newly":[{
                "to": "",
                "from_str": "View newly formed U.S. businesses"
            }]
    },
        "renames" : { },
        "blocks" : { }
    }]
}