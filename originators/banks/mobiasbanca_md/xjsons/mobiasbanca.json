{
    "name": "mobiasbanca",
    "test_url" : "http://www.mobiasbanca.md/en",
    "version": 1,
    "versions": [{
        "version" : 1,
        "timestamp" : 1332498235,
        "config" : {
            "xpath" : {
                "root" : "//html:html/html:body//html:div[@id='exchange-rates-block']",
                "namespaces" : {
                    "html": "http://www.w3.org/1999/xhtml"
                }
            },
            "anomaly" : {
                "root" : "//html:html/html:body",
                "namespaces" : { }
            }
        },
        "anomaly" : {},
        "light_handlers" : {
            "^html:" : {},
            "^date:" : ["%d/%m/%Y"]
        },
        "rules" : {
            "date" : ["text.|^date:|mandatory", ".//html:span[@class='exchange-date']"],
            "items" : ["mandatory", ".//html:div[@id='mobiasbanca-exchange']/html:table[@id='official-rates-table']", {
                "usd" : ["mandatory", ".//html:td[text()='USD' and @class='symbol-decreased']/..", "=>buy_sell"],
                "eur" : ["mandatory", ".//html:td[text()='EUR' and @class='symbol-decreased']/..", "=>buy_sell"],
                "rub" : ["mandatory", ".//html:td[text()='RUB' and @class='symbol-decreased']/..", "=>buy_sell"]
            }]
        },
        "blocks" : {
            "buy_sell" : {
                "buy"  : ["float.|mandatory", "./html:td[@class='buy']"],
                "sell" : ["float.|mandatory", "./html:td[@class='sell last']"]
            }
        },
        "reformat" : []
    }]
}

